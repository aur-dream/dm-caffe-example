# GoogLeNet solver
# When setting max_iter use this formula: max_iter = epochs * (dataset_size / batch_size)
net: "/train.prototxt"
display: 1
average_loss: 7
base_lr: 0.001
lr_policy: "step"
stepsize: 7
gamma: 0.96
max_iter: 7
momentum: 0.9
weight_decay: 0.0002
snapshot: 7
snapshot_prefix: "/modelState/caffe_googlenet"
solver_mode: GPU