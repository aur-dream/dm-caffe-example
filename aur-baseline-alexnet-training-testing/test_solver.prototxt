# AlexNet solver
# When setting max_iter use this formula: max_iter = epochs * (dataset_size / batch_size)
net: "/train.prototxt"
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 7
display: 1
max_iter: 7
momentum: 0.9
weight_decay: 0.0005
snapshot: 7
snapshot_prefix: "/modelState/caffe_alexnet"
solver_mode: GPU
